{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [
                120
            ]
        },
        "quotemark": [true, "double"],
        "trailing-comma": false,
        "arrow-parens": false,
        "no-bitwise": false,
        "object-literal-sort-keys": false,
        "no-console": false
    }
}